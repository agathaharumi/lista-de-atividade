funcao inicio()

def máximo(a, b):
    if a > b:
    return else:
        return b

    print(f"máximo(5,6) == 6 -> obtido: {máximo(5,6)}")

    print(f"máximo(2,1) == 2 -> obtido: {máximo(2,1)}")

    print(f"máximo(7,7) == 7 -> obtido: {máximo(7,7)}")

   }
 }  
18.programa

funcao inicio()
{

real valor,maiorValor, menorValor
inteiro cont
cont=0
maiorValor=0
menorValor=0
escreva("Digite um valor:")
leia(valor)
enquanto(valor >= 0){

se(cont==0){
    maiorValor=valor
    menorValor=valor
    cont++
    }
    
    se(valor > maiorValor){
     maiorValor = valor
     }
     
     se(valor < menorValor){
       menorValor = valor
       }
       
       escreva("\nDigite um valor:")
       leia(valor)
       }
       escreva("\nNo menor valor digitado foi", menorValor)
       escreva("\nNo maior valor digitado foi:", maiorValor)
       }
   }    
